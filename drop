Auto_Drop.Name = "Auto_Drop"
Auto_Drop.Parent = Auto
Auto_Drop.BackgroundColor3 = Color3.fromRGB(255, 166, 11)
Auto_Drop.BorderSizePixel = 0
Auto_Drop.Position = UDim2.new(0.0193084572, 0, 0.0451107882, 0)
Auto_Drop.Size = UDim2.new(0.285336107, 0, 0.100407675, 0)
Auto_Drop.Font = Enum.Font.GothamBlack
Auto_Drop.Text = "Auto Drop(OFF)"
Auto_Drop.TextColor3 = Color3.fromRGB(0, 0, 0)
Auto_Drop.TextScaled = true
Auto_Drop.TextSize = 14.000
Auto_Drop.TextWrapped = true
Auto_Drop.MouseButton1Click:connect(function()
	local plr = game:GetService('Players').LocalPlayer
	
	repeat wait() until plr.Character:FindFirstChild('Humanoid') or plr.Character:FindFirstChild('xxx')
	local hum = plr.Character:FindFirstChild('Humanoid')
	
	Auto_Drop.Text = "Auto Drop(ON)"
	
	while wait() do
		if plr.Character:FindFirstChild('Humanoid') or plr.Character:FindFirstChild('xxx') then
			wait(.1)
			if plr.DataFolder.Currency.Value >= 10000 then 
				local AMT = 10000
				game.ReplicatedStorage.MainEvent:FireServer("DropMoney",AMT)
			elseif plr.DataFolder.Currency.Value <= 100 then 
				wait()
			else 
				local AMT = plr.DataFolder.Currency.Value
				game.ReplicatedStorage.MainEvent:FireServer("DropMoney",AMT)
			end
			if Auto_Drop.Text == "Auto Drop(ON)" and hum.Health > 0 then
				Auto_Drop.Text = "Auto Drop(OFF)"
				break
			end
		end
	end
end)

Anti_AFK.Name = "Anti_AFK"
Anti_AFK.Parent = Auto
Anti_AFK.BackgroundColor3 = Color3.fromRGB(255, 166, 11)
Anti_AFK.BorderSizePixel = 0
Anti_AFK.Position = UDim2.new(0.341116071, 0, 0.0451107882, 0)
