local function HMPKPU_fake_script()
	local script = Instance.new('LocalScript', PinCode)

	local PinCode = script.Parent.Pinbox
	local Pinframe = script.Parent.Parent.PinCode
	local Comfirm = script.Parent.Comfirm
	local frame = script.Parent.Parent.Auto
	
	Comfirm.MouseButton1Click:Connect(function()
		if PinCode.Text == "12345" then
			Comfirm.Text = "Correct Code."
			wait(1)
			frame.Visible = true
			Pinframe.Visible = false
		else
			Comfirm.Text = "Wrong Code."
			wait(1)
			Comfirm.Text = "Correct?"
		end
	end)
end
coroutine.wrap(HMPKPU_fake_script)()
local function SANSJJ_fake_script()
	local script = Instance.new('LocalScript', Main)

	local frame = script.Parent.Parent.Main
	local hotkey = Enum.KeyCode.V
	
	local UIS = game:GetService("UserInputService")
	local open = false
	
	UIS.InputBegan:Connect(function(key, gp)
		if key.KeyCode == hotkey then
			if UIS:GetFocusedTextBox() == nil then
				if open == false then
					open = true 
					frame.Visible = open
				elseif open == true then
					open = false
					frame.Visible = open
				end
			end
		end
	end)
end
coroutine.wrap(SANSJJ_fake_script)()
local function BVWNY_fake_script()
	local script = Instance.new('LocalScript', Delete)

	local main = script.Parent.Parent.Parent.Main
	
	script.Parent.MouseButton1Click:Connect(function()
		main.Visible = false
	end)
end
coroutine.wrap(BVWNY_fake_script)()
